// 4 WAY MUX
//S0 	S1 	A 	B 	C 	D 	Out
//0 	0 	0 	X 	X 	X 	0
//0 	0 	1 	X 	X 	X 	1
//0 	1 	X 	0 	X 	X 	0
//0 	1 	X 	1 	X 	X 	1
//1 	0 	X 	X 	0 	X 	0
//1 	0 	X 	X 	1 	X 	1
//1 	1 	X 	X 	X 	0 	0
//1 	1 	X 	X 	X 	1 	1

/**
 * 4-way multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way {
    IN a, b, c, d, sel[2];
    OUT out;


}